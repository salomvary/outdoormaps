{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  "overrides": [
    {
      "files": ["*-test.ts"],
      "env": {
        "mocha": true
      },
      "rules": {
        "no-unused-expressions": 0
      }
    }
  ]
}
